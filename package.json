{
  "name": "admin-panel-template-reactjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rematch/core": "^1.4.0",
    "@rematch/persist": "^1.1.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-persist": "^4.3.1",
    "axios": "^0.19.2",
    "history": "^5.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "typescript": "~3.7.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "src/**/*.{json,css,scss,sass}": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "npm": "please use yarn",
    "yarn": ">= 1.19.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:stg": "env-cmd -f .env.stg react-app-rewired start",
    "start:prod": "env-cmd -f .env.prod react-app-rewired start",
    "build": "react-app-rewired build",
    "build:stg": "env-cmd -f .env.stg react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "test:stg": "env-cmd -f .env.stg react-app-rewired test",
    "test:prod": "env-cmd -f .env.prod react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier 'src/**/*.{ts,tsx,json,scss}' --write",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix --quiet"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "customize-cra-react-refresh": "^1.1.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6"
  }
}
